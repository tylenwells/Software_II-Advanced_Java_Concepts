<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="LoginPage">
    <children>
        <Label fx:id="loginbannertext" alignment="CENTER" layoutX="22.0" layoutY="14.0" text="Scheduling Application Login" textAlignment="CENTER">
            <font>
                <Font name="Calibri" size="22.0" />
            </font>
        </Label>
        <TextField fx:id="usernamefield" layoutX="110.0" layoutY="117.0" />
        <Label fx:id="usernametext" layoutX="42.0" layoutY="121.0" text="Username:" />
        <Label fx:id="passwordtext" layoutX="44.0" layoutY="155.0" text="Password:" />
        <Button fx:id="loginbutton" layoutX="214.0" layoutY="188.0" mnemonicParsing="false" onAction="#loginbuttonpressed" text="Login" />
        <Button fx:id="enlangbutton" layoutX="124.0" layoutY="361.0" mnemonicParsing="false" onAction="#enlangbuttonpressed" text="EN (English)" />
        <Button fx:id="jplangbutton" layoutX="214.0" layoutY="361.0" mnemonicParsing="false" onAction="#jplangbuttonpressed" text="JP (日本語)" />
        <PasswordField fx:id="passwordfield" layoutX="110.0" layoutY="151.0" />
    </children>
</AnchorPane>
